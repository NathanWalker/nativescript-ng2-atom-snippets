# NativeScript + Angular 2 Snippets for TypeScript (.ts) and HTML (.html) files

'.source.ts' :
################################################################################
  '{N} Component with template':
    'prefix': 'nscomponent_template'
    'body': """
      import {Component} from '@angular/core';

      @Component({
        selector: '${1:selector}',
        template: `
          <StackLayout>
            <Label text="Sample"></Label>
          </StackLayout>
        `
      })
      export class ${2:ClassName} {

        constructor() {

        }
      }
    """

  '{N} Component with templateUrl':
    'prefix': 'nscomponent_templateUrl'
    'body': """
      import {Component} from '@angular/core';

      @Component({
        selector: '${1:selector}',
        templateUrl: `${2:templateUrl}`
      })
      export class ${3:ClassName} {

        constructor() {

        }
      }
    """
## Action Bar #####################################################################
  "{N} Action Bar":
    "prefix": "nsactionbar"
    "body": """
      <ActionBar title="Title" icon="">
        <ActionItem (tap)="$1">
          <Button class="" text="Left"></Button>
        </ActionItem>
        <ActionItem (tap)="$2" ios.position="right">
          <Button class="" text="Right"></Button>
        </ActionItem>
      </ActionBar>
    """

  "{N} Action Bar with Back":
    "prefix": "nsactionbarback"
    "body": """
      <ActionBar title="Title" icon="">
        <NavigationButton text="Back" android.systemIcon="ic_menu_back"></NavigationButton>
        <ActionItem (tap)="$1" ios.position="right">
          <Button class="" text="Right" (tap)=""></Button>
        </ActionItem>
      </ActionBar>
    """

  "{N} Action Bar with Custom Title":
    "prefix": "nsactionbarcustomtitle"
    "body": """
      <ActionBar>
        <StackLayout orientation="horizontal"
          ios:horizontalAlignment="center"
          android:horizontalAlignment="left">
          <Label text="$1"></Label>
        </StackLayout>
        <ActionItem (tap)="$2" ios.position="right">
          <Button class="" text="Right"></Button>
        </ActionItem>
      </ActionBar>
    """

  # Layouts
  "{N} Stack Layout":
    "prefix": "nsstack"
    "body": """
      <StackLayout>
        $1
      </StackLayout>
    """

  "{N} Stack Layout Horizontal":
    "prefix": "nsstack_horizontal"
    "body": """
      <StackLayout orientation="horizontal">
        $1
      </StackLayout>
    """

  "{N} Wrap Layout":
    "prefix": "nswrap"
    "body": """
      <WrapLayout orientation="horizontal" itemWidth="$1" itemHeight="$2">
        $3
      </WrapLayout>
    """

  "{N} Absolute Layout":
    "prefix": "nsabsolute"
    "body": """
      <AbsoluteLayout>
        <Label text="This is Label 1" top="" right="" bottom="" left=""></Label>
        $1
      </AbsoluteLayout>
    """

  "{N} Dock Layout":
