          <Label text="2, 1" row="2" col="1"></Label>
          <Label text="2, 2" row="2" col="2"></Label>
          <Label text="2, 3" row="2" col="3"></Label>
          <Label text="3, 0" row="3" col="0"></Label>
          <Label text="3, 1" row="3" col="1"></Label>
          <Label text="3, 2" row="3" col="2"></Label>
          <Label text="3, 3" row="3" col="3"></Label>
      </GridLayout>
    """


  # Views
  "{N} Scroll View":
    "prefix": "nsscrollview"
    "body": """
      <ScrollView>
          $1
      </ScrollView>
    """

  "{N} List View":
    "prefix": "nslistview"
    "body": """
      <ListView [items]="source" (loaded)="onLoaded($event)" (itemLoading)="onItemLoading($event)" (itemTap)="onItemTap($event)">
          <template #item="item" #odd="odd" #even="even">
              $1
          </template>
      </ListView>
    """

  "{N} Tab View":
    "prefix": "nstabview"
    "body": """
      <TabView #tabView>
        <TabViewItem title="Tab 1">
          <Label text="This is Label in Tab 1"></Label>
        </TabViewItem>
        <TabViewItem title="Tab 2">
          <Label text="This is Label in Tab 2"></Label>
        </TabViewItem>
      </TabView>
      $1
    """

  "{N} Web View":
    "prefix": "nswebview"
    "body": """
      <WebView src="someUrl | pathToLocalFile | htmlString"></WebView>
      $1
    """


  # Widgets
  "{N} Image":
    "prefix": "nsimg"
    "body": """
      <Image src="$1"></Image>
      $2
    """

  "{N} Label":
    "prefix": "nslabel"
    "body": """
      <Label text="$1" textWrap="true"></Label>
      $2
    """

  "{N} Button":
    "prefix": "nsbtn"
    "body": """
      <Button text="$1" (tap)="$2"></Button>
      $3
    """

  "{N} Text Field":
    "prefix": "nstextfield"
    "body": """
      <TextField hint="$1" text="$2"></TextField>
      $3
    """

  "{N} Password Field":
    "prefix": "nspassword"
    "body": """
      <TextField hint="Enter your password." text="$1" secure="true"></TextField>
      $2
    """

  "{N} Editable TextView":
    "prefix": "nstextview_editable"
    "body": """
      <TextView hint="$1" text="$2" editable="true"></TextView>
      $3
    """

  "{N} Non-Editable TextView":
    "prefix": "nstextview_non-editable"
    "body": """
      <TextView text="$1" editable="false"></TextView>
      $2
    """

  "{N} SearchBar":
    "prefix": "nssearchbar"
    "body": """
      <SearchBar id="${1:searchBar}" hint="${2:Search}" text="$3" (clear)="${4:onClear}" (submit)="${5:onSubmit}"></SearchBar>
      $6
    """

  "{N} Switch":
    "prefix": "nsswitch"
    "body": """
      <Switch [checked]="$1"></Switch>
      $2
    """

  "{N} Progress Bar":
    "prefix": "nsprogress"
    "body": """
      <Progress [value]="$1"></Progress>
      $2
    """

  "{N} Slider":
    "prefix": "nsslider"
    "body": """
      <Slider [minValue]="$1" [maxValue]="$2" [value]="$3"></Slider>
      $4
    """

  "{N} Date Picker":
    "prefix": "nsdate"
    "body": """
      <DatePicker [day]="$1" [month]="$2" [year]="$3"></DatePicker>
      $4
    """

  "{N} Time Picker":
    "prefix": "nstime"
    "body": """
      <TimePicker [hour]="$1" [minute]="$2"></TimePicker>
      $3
    """

  "{N} List Picker":
    "prefix": "nslistpicker"
    "body": """
      <ListPicker [items]="$1" [(ngModel)]="$2"></ListPicker>
      $3
    """

  "{N} Activity Indicator":
    "prefix": "nsloading"
    "body": """
      <ActivityIndicator width="100" height="100" [busy]="$1"></ActivityIndicator>
      $2
    """

  "{N} Segmented Bar":
    "prefix": "nssegmentedbar"
    "body": """
      <SegmentedBar [items]="$1" [(ngModel)]="$2">
      </SegmentedBar>
    """

  "{N} Border":
    "prefix": "nsborder"
    "body": """
      <Border borderRadius="$1" borderWidth="$2" borderColor="#000000">
        $3
      </Border>
    """


  # Attributes
  "{N} CSS Class Attribute":
    "prefix": "css"
    "body": 'class="$1"$2'

  "{N} Text Attribute":
    "prefix": "text"
    "body": 'text="$1"$2'

  "{N} Icon Attribute":
    "prefix": "icon"
    "body": 'icon="res://$1"$2'

  "{N} Tap Attribute":
    "prefix": "tap"
    "body": '(tap)="$1"$2'

	"{N} Text Wrap Attribute":
    "prefix": "wrap"
    "body": 'textWrap="true"$2'

  "{N} Horizontal Alignment Attribute":
    "prefix": "halign"
    "body": 'horizontalAlignment="$1"$2'

  "{N} Vertical Alignment Attribute":
    "prefix": "valign"
    "body": 'verticalAlignment="$1"$2'

  "{N} Visibility Attribute":
    "prefix": "visibility"
    "body": '[visibility]="isItemVisible ? \'visible\' : \'collapsed\'"$2'

  "{N} Stretch Attribute":
    "prefix": "stretch"
    "body": 'stretch="fill$1"$2'

  "{N} Keyboard Type Attribute":
    "prefix": "kbtype"
    "body": 'keyboardType="email$1"$2'

  "{N} Row Attribute":
    "prefix": "row"
    "body": 'row="$1"$2'

  "{N} Column Attribute":
    "prefix": "col"
    "body": 'col="$1"$2'

  "{N} Colspan Attribute":
    "prefix": "colspan"
    "body": 'colSpan="$1"$2'

  "{N} Rowspan Attribute":
    "prefix": "rowspan"
    "body": 'rowSpan="$1"$2'

  "{N} Width Attribute":
    "prefix": "width"
    "body": 'width="$1"$2'

  "{N} Height Attribute":
    "prefix": "height"
    "body": 'height="$1"$2'
